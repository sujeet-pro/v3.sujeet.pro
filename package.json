{
  "name": "v3-sujeet-pro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run clean && npm run sync && npm run build && npm run dev",
    "dev": "astro dev",
    "dev:gh": "astro dev --site 'http://projects.sujeet.pro' --base '/v3.sujeet.pro'",
    "build": "astro check && astro build",
    "build:gh": "astro check && astro build --site 'http://projects.sujeet.pro' --base '/v3.sujeet.pro'",
    "clean": "rm -rf dist .astro",
    "preview": "astro preview",
    "astro": "astro",
    "sync": "astro sync",
    "format": "prettier --write  \"**/*.{js,jsx,ts,tsx,md,mdx,astro}\"",
    "lint": "eslint --fix \"src/**/*.{js,ts,jsx,tsx,astro}\""
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.0.7",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.2.1",
    "@fontsource-variable/open-sans": "^5.1.1",
    "@rehype-pretty/transformers": "^0.13.2",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^22.10.10",
    "@typescript-eslint/parser": "^8.21.0",
    "astro": "^5.1.9",
    "eslint": "^9.19.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "json5": "^2.2.3",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-config-standard": "^7.0.0",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rehype-pretty-code": "^0.14.0",
    "shiki": "^1.29.1",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3"
  }
}
