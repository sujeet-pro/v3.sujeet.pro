{
  "name": "v3-sujeet-pro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run clean && npm run sync && npm run build && npm run dev",
    "dev": "astro dev",
    "dev:gh": "astro dev --site 'http://projects.sujeet.pro' --base '/v3.sujeet.pro'",
    "build": "astro check && astro build",
    "build:gh": "astro check && astro build --site 'http://projects.sujeet.pro' --base '/v3.sujeet.pro'",
    "clean": "rm -rf dist .astro",
    "preview": "astro preview",
    "astro": "astro",
    "sync": "astro sync",
    "format": "prettier --write  \"**/*.{js,jsx,ts,tsx,md,mdx,astro}\"",
    "lint": "eslint --fix \"src/**/*.{js,ts,jsx,tsx,astro}\""
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.0.3",
    "@astrojs/rss": "^4.0.10",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/tailwind": "^5.1.4",
    "@fontsource-variable/open-sans": "^5.1.0",
    "@tailwindcss/typography": "^0.5.15",
    "astro": "^5.1.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "prettier": "^3.4.2",
    "prettier-config-standard": "^7.0.0",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.9"
  }
}
