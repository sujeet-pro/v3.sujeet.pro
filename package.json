{
  "name": "v3-sujeet-pro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run clean && npm run sync && npm run build && npm run dev",
    "start:preview": "npm run clean && npm run sync && npm run build && npm run preview",
    "start:preview:gh": "npm run clean && npm run sync && npm run build:gh && npm run preview:gh",
    "dev": "astro dev",
    "dev:gh": "astro dev --site 'http://projects.sujeet.pro' --base '/v3.sujeet.pro/'",
    "build": "astro check && astro build",
    "build:gh": "astro check && astro build --site 'http://projects.sujeet.pro' --base '/v3.sujeet.pro/'",
    "preview": "astro preview",
    "preview:gh": "astro preview --site 'http://projects.sujeet.pro' --base '/v3.sujeet.pro/'",
    "clean": "rm -rf dist .astro",
    "astro": "astro",
    "sync": "astro sync",
    "format": "prettier --write  \"**/*.{js,jsx,ts,tsx,md,mdx,astro}\"",
    "lint": "eslint --fix \"src/**/*.{js,ts,jsx,tsx,astro}\""
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.0.8",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.2.1",
    "@fontsource-variable/open-sans": "^5.1.1",
    "@mermaid-js/mermaid-cli": "^11.4.2",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.7",
    "@types/node": "^22.13.4",
    "@typescript-eslint/parser": "^8.24.1",
    "astro": "^5.3.0",
    "daisyui": "^5.0.0-beta.8",
    "eslint": "^9.20.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "json5": "^2.2.3",
    "postcss": "^8.5.2",
    "prettier": "^3.5.1",
    "prettier-config-standard": "^7.0.0",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.7",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@expressive-code/plugin-collapsible-sections": "^0.40.2",
    "@expressive-code/plugin-line-numbers": "^0.40.2",
    "@microflash/rehype-figure": "^2.1.3",
    "astro-expressive-code": "^0.40.2",
    "astro-pagefind": "^1.8.0",
    "rehype-external-links": "^3.0.0"
  }
}
