    "@fontsource-variable/open-sans": "^5.1.0",
    "@tailwindcss/typography": "^0.5.15",
    "astro": "^5.0.0-beta.8",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@types/node": "^22.9.1",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "prettier": "^3.3.3",
    "prettier-config-standard": "^7.0.0",
